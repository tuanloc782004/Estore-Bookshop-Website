public interface CartService {

	public void deleteById(Long id);

	public List<Category> findByKeyword(String keyword, String status, LocalDateTime varDate);

	public Page<Category> findAll(Integer pageno);

	public Page<Category> findByKeyword(String keyword, String status, LocalDateTime varDate, Integer pageNo);
	
}
